{
    "name": "librenms/weathermapng",
    "description": "Next-generation Weathermap plugin for LibreNMS",
    "type": "librenms-plugin",
    "version": "1.0.0",
    "license": "Unlicense",
    "authors": [
        {
            "name": "LibreNMS Community",
            "email": "info@librenms.org"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-gd": "*",
        "ext-json": "*",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/http": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "LibreNMS\\Plugins\\WeathermapNG\\": ".",
            "LibreNMS\\Plugins\\WeathermapNG\\Http\\Controllers\\": "Http/Controllers/",
            "LibreNMS\\Plugins\\WeathermapNG\\Models\\": "Models/",
            "LibreNMS\\Plugins\\WeathermapNG\\Services\\": "Services/",
            "LibreNMS\\Plugins\\WeathermapNG\\Policies\\": "Policies/",
            "LibreNMS\\Plugins\\WeathermapNG\\RRD\\": "lib/RRD/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LibreNMS\\Plugins\\WeathermapNG\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpcs --standard=PSR12 Http/ Models/ Services/",
        "lint-fix": "phpcbf --standard=PSR12 Http/ Models/ Services/"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "librenms": {
            "plugin-name": "WeathermapNG",
            "version": "1.0.0",
            "description": "Modern interactive network weathermap for LibreNMS",
            "author": "LibreNMS Community",
            "email": "info@librenms.org",
            "homepage": "https://github.com/lance0/weathermapNG"
        }
    }
}
